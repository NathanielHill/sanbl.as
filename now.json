{
  "name": "sanblas",
  "alias": "sanbl.as",
  "version": 2,
  "builds": [
    { "src": "public/*", "use": "@now/static" },
    { "src": "www/package.json", "use": "@now/static-build" }
  ],
  "routes": [
    { "src": "^/favicon.ico", "dest": "/public/favicon.ico" },
    { "src": "^/robots.txt", "dest": "/public/robots.txt" },
    { "src": "^/(.*)", "dest": "/www/$1" }
  ]
}
